    "editorSuggestWidget.selectedBackground": "#21252b",
    "editorWhitespace.foreground": "#3b4048",
    "editorWidget.background": "#2f343f",
    "foreground": "#fffbdd",
    "gitDecoration.ignoredResourceForeground": "#89949a",
    "input.background": "#00003C",
    "input.border": "#00003C",
    "input.foreground": "#fffbdd",
    "input.placeholderForeground": "#e4eef4",
    "list.activeSelectionBackground": "#434340",
    "list.activeSelectionForeground": "#fed506",
    "list.dropBackground": "#2F2F2B",
    "list.focusBackground": "#434340",
    "list.highlightForeground": "#c5c5c5",
    "list.hoverBackground": "#434340",
    "list.inactiveSelectionBackground": "#434340",
    "list.inactiveSelectionForeground": "#fed506",
    "peekView.border": "#2F2F2B",
